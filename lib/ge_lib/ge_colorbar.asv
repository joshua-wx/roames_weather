function ge_colorbar
A = double(imread('colorbar_template.bmp'));
A=A./256;
radar_colormap=flipud([A(:,:,1),A(:,:,2),A(:,:,3)]);
colormap(radar_colormap);
colorbar('YTickLabel',...
    {'10','20','30','40',...
     '50','60','70'},'FontWeight','bold');
axis off
a_map=ones(length(A(:,:,1)),1);
a_map(10)=0;
set(gcf,'Alphamap',a_map,'color',)
saveas(gca,'colorbar.png')



